msgid "No files found."
msgstr "Geen bestanden gevonden."

#: ../catfish/CatfishWindow.py:1249 ../catfish/CatfishWindow.py:1483
msgid "1 file found."
msgstr "1 bestand gevonden."

#: ../catfish/CatfishWindow.py:1252 ../catfish/CatfishWindow.py:1485
#, python-format
msgid "%i files found."
msgstr "%i bestanden gevonden."

#: ../catfish/CatfishWindow.py:1260
