msgid "<b>File Type</b>"
msgstr "<b>Bestandtype</b>"

#: ../data/ui/CatfishWindow.ui.h:19
msgid "Custom Time Range"
msgstr "Aangepast tijdsbestek"

#: ../data/ui/CatfishWindow.ui.h:20
msgid "Go to Today"
msgstr "Ga naar vandaag"

