#: ../catfish.desktop.in.h:1 ../catfish/CatfishWindow.py:374
msgid "Catfish File Search"
msgstr "Catfish-tiedostohaku"

#: ../catfish.desktop.in.h:2
msgid "Search the file system"
msgstr "Hae tiedostojärjestelmästä"

#: ../data/ui/AboutCatfishDialog.ui.h:1
msgid "Catfish is a versatile file searching tool."
msgstr "Catfish on monipuolinen tiedostonhakutyökalu."

#: ../data/ui/CatfishWindow.ui.h:1
msgid "Catfish"
msgstr "Catfish"

#: ../data/ui/CatfishWindow.ui.h:2
msgid "Select a Directory"
msgstr "Valitse kansio"

#: ../data/ui/CatfishWindow.ui.h:3 ../catfish/CatfishWindow.py:462
#: ../catfish/CatfishWindow.py:1417
msgid "Enter search terms and press ENTER"
msgstr "Syötä hakusanat ja paina ENTER"

#: ../data/ui/CatfishWindow.ui.h:4
msgid "Search terms"
msgstr "Hakuehdot"

#: ../data/ui/CatfishWindow.ui.h:5
