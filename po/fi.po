msgstr "Koska tahansa"

#: ../data/ui/CatfishWindow.ui.h:6
msgid "This week"
msgstr "Tällä viikolla"

#: ../data/ui/CatfishWindow.ui.h:7
msgid "Custom"
msgstr "Mukautettu"

#: ../data/ui/CatfishWindow.ui.h:8
msgid "<b>Modified</b>"
msgstr "<b>Muokattu</b>"

#: ../data/ui/CatfishWindow.ui.h:9
msgid "Documents"
msgstr "Asiakirjat"

#: ../data/ui/CatfishWindow.ui.h:10
msgid "Folders"
msgstr "Kansiot"

#: ../data/ui/CatfishWindow.ui.h:11
msgid "Images"
msgstr "Kuvat"

#: ../data/ui/CatfishWindow.ui.h:12
msgid "Music"
msgstr "Musiikki"

#: ../data/ui/CatfishWindow.ui.h:13
msgid "Videos"
msgstr "Videot"

#: ../data/ui/CatfishWindow.ui.h:14
msgid "Applications"
msgstr "Sovellukset"

#: ../data/ui/CatfishWindow.ui.h:15
msgid "Other"
msgstr "Muu"

#: ../data/ui/CatfishWindow.ui.h:16
msgid "<b>File Type</b>"
msgstr "<b>Tiedostotyyppi</b>"

#: ../data/ui/CatfishWindow.ui.h:17
msgid "Custom Time Range"
msgstr "Mukautettu aikaväli"

#: ../data/ui/CatfishWindow.ui.h:18
msgid "Go to Today"
msgstr "Siirry tähän päivään"

#: ../data/ui/CatfishWindow.ui.h:19
msgid "<b>Start Date</b>"
msgstr "<b>Aloituspäivä</b>"

#: ../data/ui/CatfishWindow.ui.h:20
msgid "<b>End Date</b>"
msgstr "<b>Päättymispäivä</b>"

#: ../data/ui/CatfishWindow.ui.h:21
msgid "Custom File Type"
msgstr "Mukautettu tiedostotyyppi"

#. The "mimetype" is the file type of the file, such as "application/octet-stream"
#: ../data/ui/CatfishWindow.ui.h:23
msgid "Select existing mimetype"
msgstr "Valitse olemassa oleva MIME-tyyppi"

#. Enter "file extensions" such as .doc, .pdf
#: ../data/ui/CatfishWindow.ui.h:25
msgid "Enter file extensions"
msgstr "Syötä tiedostotunnisteet"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:27
msgid "_Exact Match"
msgstr "_Tarkka osuma"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:29
msgid "_Hidden Files"
msgstr "_Piilotiedostot"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:31
msgid "_Fulltext Search"
msgstr "_Kokotekstihaku"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:33
msgid "_Show Advanced Settings"
msgstr "Näytä _lisäasetukset"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:35
msgid "_Update Search Index…"
msgstr "Päivitä _hakuindeksi"

#: ../data/ui/CatfishWindow.ui.h:36
msgid "_About"
msgstr "_Tietoja"

#: ../data/ui/CatfishWindow.ui.h:37
msgid "_Open"
msgstr "_Avaa"

#. This menu contains the menu items: _Open, Show in _File Manager, _Copy Location, Save _As, _Delete
#: ../data/ui/CatfishWindow.ui.h:39
msgid "Show in _File Manager"
msgstr "_Näytä tiedostoselaimessa"

#. This menu contains the menu items: _Open, Show in _File Manager, _Copy Location, Save _As, _Delete
#: ../data/ui/CatfishWindow.ui.h:41
msgid "_Copy Location"
msgstr "Kopioi _sijainti"

#: ../data/ui/CatfishWindow.ui.h:42
msgid "_Save as..."
msgstr "_Tallenna nimellä..."

#: ../data/ui/CatfishWindow.ui.h:43
msgid "_Delete"
msgstr "_Poista"

#: ../data/ui/CatfishWindow.ui.h:44
msgid "Update Search Index"
msgstr "Päivitä hakuindeksi"

#: ../data/ui/CatfishWindow.ui.h:45
msgid "Unlock"
msgstr "Avaa lukitus"

#: ../data/ui/CatfishWindow.ui.h:46
msgid "<big><b>Update Search Index</b></big>"
msgstr "<big><b>Päivitä hakuindeksi</b></big>"

#. The term "locate" is a commandline application, so it should not be translated.
#: ../data/ui/CatfishWindow.ui.h:48
msgid ""
"To provide accurate results, the <i>locate</i> database needs to be "
"refreshed.\n"
"This requires sudo (admin) rights."
msgstr ""
"Saadaksesi täsmällisiä tuloksia, <i>locate</i>-tietokanta pitää päivittää.\n"
"Tämä vaatii ylläpitäjän sudo-oikeudet."

#: ../data/ui/CatfishWindow.ui.h:50
msgid "Updating database…"
msgstr "Päivitetään tietokantaa..."

#: ../data/ui/CatfishWindow.ui.h:51
msgid "Done."
msgstr "Valmis."

#: ../catfish/__init__.py:37
msgid "Show debug messages (-vv debugs catfish_lib also)"
msgstr ""
"Näytä virheilmoitukset (-vv näyttää myös catfish_libin virheilmoitukset)"

#: ../catfish/__init__.py:40
msgid "Use large icons"
msgstr "Käytä suuria kuvakkeita"

#: ../catfish/__init__.py:42
msgid "Use thumbnails"
msgstr "Käytä esikatselua"

#: ../catfish/__init__.py:44
msgid "Display time in ISO format"
msgstr "Näytä aika ISO-muodossa"

#. Translators: Do not translate PATH, it is a variable.
#: ../catfish/__init__.py:46
msgid "Search in folder PATH"
msgstr "Hae hakemistosta PATH"

#. Translators: Do not translate FILEMAN, it is a variable.
#: ../catfish/__init__.py:48
msgid "Use FILEMAN as filemanager"
msgstr "Käytä sovellusta FILEMAN tiedostoselaimena"

#. Translators: Do not translate WRAPPER, it is a variable
#: ../catfish/__init__.py:51
msgid "Use WRAPPER to open files"
msgstr "Käytä sovellusta WRAPPER tiedostojen avaamiseen"

#: ../catfish/__init__.py:53
msgid "Perform exact match"
msgstr "Suorita tarkka haku"

#: ../catfish/__init__.py:55
msgid "Include hidden files"
msgstr "Sisällytä piilotiedostot"

#: ../catfish/__init__.py:57
msgid "Perform fulltext search"
msgstr "Suorita kokotekstihaku"

#. Translators: this text is displayed next to
#. a filename that is not utf-8 encoded.
#: ../catfish/CatfishWindow.py:84
#, python-format
msgid "%s (invalid encoding)"
msgstr "%s (virheellinen merkistökoodaus)"

#: ../catfish/CatfishWindow.py:319
msgid "Preview"
msgstr "Esikatselu"

#: ../catfish/CatfishWindow.py:322 ../catfish/CatfishWindow.py:326
msgid "Filename"
msgstr "Tiedoston nimi"

#: ../catfish/CatfishWindow.py:328
msgid "Size"
msgstr "Koko"

#: ../catfish/CatfishWindow.py:330
msgid "Location"
msgstr "Sijainti"

#: ../catfish/CatfishWindow.py:332
msgid "Last modified"
msgstr "Viimeksi muokattu"

#: ../catfish/CatfishWindow.py:381 ../catfish/CatfishWindow.py:411
msgid "Authentication failed."
msgstr "Tunnistus epäonnistui."

#: ../catfish/CatfishWindow.py:405 ../catfish/CatfishWindow.py:413
msgid "Locate database updated successfully."
msgstr "Locate-tietokanta päivitettiin onnistuneesti."

#: ../catfish/CatfishWindow.py:407
msgid "An error occurred while updating locatedb."
msgstr "Virhe päivitettäessä locate-tietokantaa."

#: ../catfish/CatfishWindow.py:409
msgid "User aborted authentication."
msgstr "Käyttäjä keskeytti tunnistuksen."

#: ../catfish/CatfishWindow.py:465 ../catfish/CatfishWindow.py:1424
msgid "Clear search terms"
msgstr "Tyhjennä hakusanat"

#. Translators: This message will only appear when trying to
#. open the default file manager but it cannot be found.
#: ../catfish/CatfishWindow.py:792
msgid "Catfish could not find the default file manager."
msgstr "Catfish ei löytänyt oletustiedostoselainta."

#. Translators: This message will only appear when the default
#. application for opening a file cannot be found.
#: ../catfish/CatfishWindow.py:799
msgid "Catfish could not find the default open wrapper."
msgstr "Catfish ei löytänyt oletusohjelmaa tiedoston avaamiseen."

#: ../catfish/CatfishWindow.py:813
#, python-format
msgid "\"%s\" could not be opened."
msgstr "Tiedostoa \"%s\" ei voitu avata."

#: ../catfish/CatfishWindow.py:859
#, python-format
msgid "\"%s\" could not be saved."
msgstr "Tiedostoa \"%s\" ei voitu tallentaa."

#: ../catfish/CatfishWindow.py:884
#, python-format
msgid "\"%s\" could not be deleted."
msgstr "Tiedostoa \"%s\" ei voitu poistaa."

#: ../catfish/CatfishWindow.py:896
#, python-format
msgid "Save \"%s\" as…"
msgstr "Tallenna \"%s\" nimellä..."

#: ../catfish/CatfishWindow.py:923
#, python-format
msgid ""
"Are you sure that you want to \n"
"permanently delete \"%s\"?"
msgstr ""
"Haluatko varmasti poistaa \n"
"kohteen \"%s\" pysyvästi?"

#: ../catfish/CatfishWindow.py:927
#, python-format
msgid ""
"Are you sure that you want to \n"
"permanently delete the %i selected files?"
msgstr ""
"Haluatko varmasti poistaa \n"
"%i tiedostoa pysyvästi?"

#: ../catfish/CatfishWindow.py:930
msgid "If you delete a file, it is permanently lost."
msgstr "Jos poistat tiedoston, sitä ei voi palauttaa."

#: ../catfish/CatfishWindow.py:1154 ../catfish/CatfishWindow.py:1404
msgid "No files found."
msgstr "Tiedostoja ei löytynyt."

#: ../catfish/CatfishWindow.py:1156 ../catfish/CatfishWindow.py:1406
msgid "1 file found."
msgstr "Löydettiin 1 tiedosto."

#: ../catfish/CatfishWindow.py:1159 ../catfish/CatfishWindow.py:1408
#, python-format
msgid "%i files found."
msgstr "Löydettiin %i tiedostoa."

#: ../catfish/CatfishWindow.py:1305
#, python-format
msgid "Searching for \"%s\""
msgstr "Etsitään \"%s\""

#: ../catfish/CatfishWindow.py:1307
msgid "Searching…"
msgstr "Haetaan…"

#: ../catfish/CatfishWindow.py:1311
msgid "Stop Search"
msgstr "Keskeytä haku"

#: ../catfish/CatfishWindow.py:1399
#, python-format
msgid "Search results for \"%s\""
msgstr "Hakutulokset haulle \"%s\""
