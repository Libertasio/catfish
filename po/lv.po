#, python-format
msgid "\"%s\" could not be saved."
msgstr "Neizdevās saglabāt “%s”"

#: ../catfish/CatfishWindow.py:884
#, python-format
msgid "\"%s\" could not be deleted."
msgstr "Neizdevās izdzēst “%s”"

