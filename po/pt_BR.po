msgid "No files found."
msgstr "Nenhum arquivo encontrado."

#: ../catfish/CatfishWindow.py:1008 ../catfish/CatfishWindow.py:1220
#, python-format
msgid "%i files found."
msgstr "%i arquivos encontrados."

#: ../catfish/CatfishWindow.py:1125
#, python-format
msgid "Searching for \"%s\""
msgstr "Procurando por \"%s\""

#: ../catfish/CatfishWindow.py:1127
msgid "Searching…"
msgstr "Pesquisando…"

#: ../catfish/CatfishWindow.py:1131
msgid "Stop Search"
msgstr "Parar Busca"

