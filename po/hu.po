#: ../catfish.desktop.in.h:1
msgid "Catfish File Search"
msgstr "Catfish fájlkereső"

#: ../catfish.desktop.in.h:2
msgid "Search the file system"
msgstr "Keresés a fájlrendszerben"

#: ../data/ui/AboutCatfishDialog.ui.h:1
msgid "Catfish is a versatile file searching tool."
msgstr "A Catfish egy rugalmas fájlkereső eszköz."

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:2
msgid "_Exact Match"
msgstr "_Pontos egyezés"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:4
msgid "_Hidden Files"
msgstr "_Rejtett fájlok"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:6
msgid "_Fulltext Search"
msgstr "_Teljes szöveges keresés"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:8
msgid "_Show Advanced Settings"
msgstr "_Speciális beállítások megjelenítése"

#. This menu contains the menu items: _Exact Match, _Hidden Files, _Fulltext Search, _Show Advanced Settings, _Update Search Index, _About
#: ../data/ui/CatfishWindow.ui.h:10
msgid "_Update Search Index…"
msgstr "_Keresési index frissítése"

#: ../data/ui/CatfishWindow.ui.h:11
msgid "Catfish"
msgstr "Catfish"

#: ../data/ui/CatfishWindow.ui.h:12
msgid "Select a Directory"
msgstr "Könyvtár kiválasztása"

#: ../data/ui/CatfishWindow.ui.h:13 ../catfish/CatfishWindow.py:346
#: ../catfish/CatfishWindow.py:1229
msgid "Enter search terms and press ENTER"
msgstr "Adja meg a keresési kifejezéseket, majd nyomja meg az Entert"

#: ../data/ui/CatfishWindow.ui.h:14
msgid "Any time"
msgstr "Bármikor"

#: ../data/ui/CatfishWindow.ui.h:15
msgid "This week"
msgstr "Ezen a héten"

#: ../data/ui/CatfishWindow.ui.h:16
msgid "Custom"
msgstr "Egyéni"

#: ../data/ui/CatfishWindow.ui.h:17
msgid "<b>Modified</b>"
msgstr "<b>Módosítás</b>"

#: ../data/ui/CatfishWindow.ui.h:18
msgid "Documents"
msgstr "Dokumentumok"

#: ../data/ui/CatfishWindow.ui.h:19
msgid "Images"
msgstr "Képek"

#: ../data/ui/CatfishWindow.ui.h:20
msgid "Music"
msgstr "Zene"

#: ../data/ui/CatfishWindow.ui.h:21
msgid "Videos"
msgstr "Videók"

#: ../data/ui/CatfishWindow.ui.h:22
msgid "Applications"
msgstr "Alkalmazások"

#: ../data/ui/CatfishWindow.ui.h:23
msgid "Other"
msgstr "Egyéb"

#: ../data/ui/CatfishWindow.ui.h:24
msgid "<b>File Type</b>"
msgstr "<b>Fájltípus</b>"

#: ../data/ui/CatfishWindow.ui.h:25
msgid "Custom Time Range"
msgstr "Egyéni időintervallum"

#: ../data/ui/CatfishWindow.ui.h:26
msgid "Go to Today"
msgstr "Ugrás a mai naphoz"

#: ../data/ui/CatfishWindow.ui.h:27
msgid "<b>Start Date</b>"
msgstr "<b>Kezdő dátum</b>"

#: ../data/ui/CatfishWindow.ui.h:28
msgid "<b>End Date</b>"
msgstr "<b>Záró dátum</b>"

#: ../data/ui/CatfishWindow.ui.h:29
msgid "Custom File Type"
msgstr "Egyéni fájltípus"

#. The "mimetype" is the file type of the file, such as "application/octet-stream"
#: ../data/ui/CatfishWindow.ui.h:31
msgid "Select existing mimetype"
msgstr "Létező MIME-típus kiválasztása"

#. Enter "file extensions" such as .doc, .pdf
#: ../data/ui/CatfishWindow.ui.h:33
msgid "Enter file extensions"
msgstr "Adja meg a fájlkiterjesztéseket"

#. This menu contains the menu items: _Open, Show in _File Manager, _Copy Location, Save _As, _Delete
#: ../data/ui/CatfishWindow.ui.h:35
msgid "Show in _File Manager"
msgstr "Megjelenítés a _fájlkezelőben"

#. This menu contains the menu items: _Open, Show in _File Manager, _Copy Location, Save _As, _Delete
#: ../data/ui/CatfishWindow.ui.h:37
msgid "_Copy Location"
msgstr "Hely _másolása"

#: ../data/ui/CatfishWindow.ui.h:38
msgid "Update Search Index"
msgstr "Keresési index frissítése"

#: ../data/ui/CatfishWindow.ui.h:39
msgid "Unlock"
msgstr "Feloldás"

#: ../data/ui/CatfishWindow.ui.h:40
msgid "<big><b>Update Search Index</b></big>"
msgstr "<big><b>Keresési index frissítése</b></big>"

#. The term "locate" is a commandline application, so it should not be translated.
#: ../data/ui/CatfishWindow.ui.h:42
msgid ""
"To provide accurate results, the <i>locate</i> database needs to be "
"refreshed.\n"
"This requires sudo (admin) rights."
msgstr ""
"Pontos eredmények érdekében frissíteni kell a <i>locate</i> adatbázist.\n"
"Ehhez rendszergazdai jogok szükségesek."

#: ../data/ui/CatfishWindow.ui.h:44
msgid "Updating database…"
msgstr "Adatbázis frissítése…"

#: ../data/ui/CatfishWindow.ui.h:45
msgid "Done."
msgstr "Kész."

#: ../catfish/__init__.py:35
msgid "Show debug messages (-vv debugs catfish_lib also)"
msgstr ""
"Hibakereső üzenetek megjelenítése (a -vv a catfish_lib hibaüzeneteit is "
"megjeleníti)"

#: ../catfish/CatfishWindow.py:219
msgid "Preview"
msgstr "Előnézet"

#: ../catfish/CatfishWindow.py:222 ../catfish/CatfishWindow.py:226
msgid "Filename"
msgstr "Fájlnév"

#: ../catfish/CatfishWindow.py:228
msgid "Size"
msgstr "Méret"

#: ../catfish/CatfishWindow.py:230
msgid "Location"
msgstr "Hely"

#: ../catfish/CatfishWindow.py:232
msgid "Last modified"
msgstr "Utolsó módosítás"

#: ../catfish/CatfishWindow.py:284
msgid "Locate database updated successfully."
msgstr "A locate adatbázis sikeresen frissült."

#: ../catfish/CatfishWindow.py:286
msgid "An error occurred while updating locatedb."
msgstr "Hiba történt a locatedb frissítése közben."

#: ../catfish/CatfishWindow.py:288 ../catfish/CatfishWindow.py:292
msgid "User aborted authentication."
msgstr "A felhasználó megszakította a hitelesítést."

#: ../catfish/CatfishWindow.py:290
msgid "Authentication failed."
msgstr "A hitelesítés meghiúsult."

#: ../catfish/CatfishWindow.py:349 ../catfish/CatfishWindow.py:1236
msgid "Clear search terms"
msgstr "Keresési kifejezések törlése"

#. Translators: This message will only appear when trying to open the default file manager but it cannot be found.
#: ../catfish/CatfishWindow.py:655
msgid "Catfish could not find the default file manager."
msgstr "A Catfish nem találja az alapértelmezett fájlkezelőt."

#. Translators: This message will only appear when the default application for opening a file cannot be found.
#: ../catfish/CatfishWindow.py:661
msgid "Catfish could not find the default open wrapper."
msgstr ""
"A Catfish nem találja az alapértelmezett alkalmazást a fájl megnyitásához."

#: ../catfish/CatfishWindow.py:675
#, python-format
msgid "\"%s\" could not be opened."
msgstr "„%s” nem nyitható meg."

#: ../catfish/CatfishWindow.py:713
#, python-format
msgid "\"%s\" could not be saved."
msgstr "„%s” nem menthető."

#: ../catfish/CatfishWindow.py:738
#, python-format
msgid "\"%s\" could not be deleted."
msgstr "„%s” nem törölhető."

#: ../catfish/CatfishWindow.py:750
#, python-format
msgid "Save \"%s\" as…"
msgstr "„%s” mentése másként…"

#: ../catfish/CatfishWindow.py:777
#, python-format
msgid ""
"Are you sure that you want to \n"
"permanently delete \"%s\"?"
msgstr ""
"Biztos, hogy véglegesen törölni szeretné \n"
"a(z) „%s” fájlt?"

#: ../catfish/CatfishWindow.py:781
#, python-format
msgid ""
"Are you sure that you want to \n"
"permanently delete the %i selected files?"
msgstr ""
"Biztos, hogy véglegesen törölni szeretné\n"
"a kijelölt %i fájlt?"

#: ../catfish/CatfishWindow.py:784
msgid "If you delete a file, it is permanently lost."
msgstr "Ha töröl egy fájlt, az véglegesen elvész."

#: ../catfish/CatfishWindow.py:1006 ../catfish/CatfishWindow.py:1218
msgid "No files found."
msgstr "Nem találhatók fájlok."

#: ../catfish/CatfishWindow.py:1008 ../catfish/CatfishWindow.py:1220
#, python-format
msgid "%i files found."
msgstr "%i fájl található."

#: ../catfish/CatfishWindow.py:1125
#, python-format
msgid "Searching for \"%s\""
msgstr "„%s” keresése"

#: ../catfish/CatfishWindow.py:1127
msgid "Searching…"
msgstr "Keresés…"

#: ../catfish/CatfishWindow.py:1131
msgid "Stop Search"
msgstr "Keresés leállítása"

#: ../catfish/CatfishWindow.py:1213
#, python-format
msgid "Search results for \"%s\""
msgstr "Találatok erre: „%s”"
